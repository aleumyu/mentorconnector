{
  "name": "react-express-boilerplate",
  "version": "1.0.0",
  "description": "Codely React/Express/MySQL project boilerplate",
  "main": "yarn start",
  "repository": "https://github.com/Codely-tech/react-express-boilerplate.git",
  "author": "Codely",
  "license": "Proprietary",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel api client",
    "api": "cd api && yarn start",
    "client": "cd client && yarn start",
    "install-all": "npm-run-all --parallel install-api install-client",
    "install-api": "cd api && yarn install",
    "install-client": "cd client && yarn install"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
